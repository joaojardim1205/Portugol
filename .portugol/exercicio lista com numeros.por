programa {
  funcao inicio() {
    inteiro lista[10]
    inteiro i = 0
    
    enquanto(i < 10){
      escreva("digite um numero: \n")
      leia (lista[i])

      i += 1
    } 
    escreva("A soma de todos os numero é: ", lista[0] + lista[1] + lista[2] + lista[3] + lista[4] + lista[5] + lista[6] + lista[7]+ lista[8] + lista[9])
  }
}
